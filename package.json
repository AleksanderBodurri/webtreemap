{
  "name": "webtreemap",
  "version": "0.1.0",
  "description": "treemap visualization",
  "main": "treemap.js",
  "dependencies": {},
  "devDependencies": {
    "prettier": "^1.7.0",
    "rollup": "^0.36.3",
    "typescript": "^2.5.2",
    "uglify-js": "^2.7.4"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "fmt": "prettier --write *.ts demo/*.ts",
    "dev": "tsc --watch --module system --outFile demo/demo.js demo/demo.ts",
    "build": "tsc && rollup -f iife build/oldapi-main.js | uglifyjs --screw-ie8 > build/webtreemap.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/martine/webtreemap.git"
  },
  "keywords": [
    "treemap",
    "visualization"
  ],
  "author": "Evan Martin <evan.martin@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/martine/webtreemap/issues"
  },
  "homepage": "https://github.com/martine/webtreemap#readme",
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": false,
    "parser": "typescript"
  }
}
